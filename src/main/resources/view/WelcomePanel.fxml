<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1">
   <children>
      <VBox style="-fx-background-color: black;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <GridPane gridLinesVisible="true" style="-fx-background-color: pink;">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" prefWidth="50.0"/>
                <ColumnConstraints hgrow="SOMETIMES" prefWidth="50.0"/>
              </columnConstraints>
               <children>
                  <AnchorPane fx:id="expenseStats" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                  <AnchorPane fx:id="incomeStats" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" GridPane.columnIndex="1" />
               </children>
            </GridPane>
            <Label fx:id="welcomeMessage" alignment="CENTER" style="-fx-font-size: 25; -fx-background-color: white;" text="Label"
                   textAlignment="JUSTIFY" wrapText="true">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin></Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
